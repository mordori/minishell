export VAR="   A   B   "
echo 1. $VAR
echo 2. $VAR$VAR
echo 3. $VAR"$VAR"
echo 4. "$VAR"
echo 5. ab"$VAR"
echo 6. "$VAR"ab
echo 7. "$VAR""$VAR"
echo 8. "$VAR""$VAR"ab
echo 9. '$VAR'
echo 10. '$VAR'ab
echo 11. ab'$VAR'
echo 12. '$VAR''$VAR'
echo 13. '$VAR''$VAR'ab
echo 14. '$VAR'"$VAR"
echo 15. "$VAR"'$VAR'
echo 16. '$VAR'"$VAR"ab
echo 17. "$VAR"'$VAR'ab
echo 18. '"$VAR"'
echo 19. "'$VAR'"
echo 20. ""$VAR""$VAR""
echo 21. "$VAR$VAR"
echo 22. '$VAR$VAR'
echo 23. ''$VAR''$VAR''
echo 24. "$VAR""$VAR""$VAR"
echo 25. "'"'$VAR'"'"
echo
export VAR='"   A   B   "'
