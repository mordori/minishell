export VAR='"    G  "'
cat << eof
testing without quotes "
$VAR
$VAR$VAR
$VAR"$VAR"
"$VAR"
'$VAR'
asd"$VAR"
'$VAR'asd
"$VAR"'$VAR'
'$VAR'"$VAR"
eof
echo
