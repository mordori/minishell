export VAR='"    G    sdf "'
cat << eof
testing without quotes "
1. $VAR
2. $VAR$VAR
3. $VAR"$VAR"
4. "$VAR"
5. '$VAR'
6. asd"$VAR"
7. '$VAR'asd
8. "$VAR"'$VAR'
9. '$VAR'"$VAR"
eof
echo
