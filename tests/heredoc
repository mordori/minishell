export VAR='"    G    sdf "'
cat << eof
testing without quotes "
$VAR
$VAR$VAR
$VAR"$VAR"
"$VAR"
'$VAR'
asd"$VAR"
'$VAR'asd
"$VAR"'$VAR'
'$VAR'"$VAR"
eof
echo
export VAR="'    G1  sdf '"
cat << eof
testing without quotes '
$VAR
$VAR$VAR
$VAR"$VAR"
"$VAR"
'$VAR'
asd"$VAR"
'$VAR'asd
"$VAR"'$VAR'
'$VAR'"$VAR"
eof
echo
export VAR='"    H  a "'
cat << eof""
testing with double quotes
$VAR
$VAR$VAR
$VAR"$VAR"
"$VAR"
'$VAR'
asd"$VAR"
'$VAR'asd
"$VAR"'$VAR'
'$VAR'"$VAR"
eof
echo
export VAR='"    I  a "'
cat << eof''
testing with single quotes
$VAR
$VAR$VAR
